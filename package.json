{
  "name": "vue-directive-markdown",
  "description": "Simple Vue 2.0 directive to parse markdown using the markdown-it parser.",
  "version": "1.1.5",
  "author": "cschweda <cschweda@gmail.com>",
  "license": "MIT",
  "main": "dist/vue-directive-markdown.min.js",
  "bugs": {
    "url": "https://github.com/ICJIA/vue-directive-markdown/issues"
  },
  "scripts": {
    "clean": "npm run rimraf ./dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "dist": "webpack --config webpack.config.dist.js",
    "server": "node ./node_modules/serve/bin/serve.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ICJIA/vue-directive-markdown"
  },
  "keywords": [
    "vue",
    "markdown",
    "directive"
  ],
  "dependencies": {
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "markdown-it": "^8.3.1",
    "markdown-it-attrs": "^0.9.0",
    "markdown-it-fontawesome": "^0.2.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-github-headings": "^1.0.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-named-headers": "0.0.4",
    "markdown-it-table-of-contents": "^0.3.2",
    "serve": "^6.0.3",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
}
